@model List<HoraireList>

<h1>Liste des bateau</h1>

<table class="table">
    <thead>
        <tr>
            <th>Nom du bateau</th>
            <th>Type de fermeture</th>
            <th>Date de fermeture</th>
            <th>Date de réouverture</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var boat in Model)
        {
            <tr>
                <td>@boat.boatName</td>
                <td>@boat.closingType</td>
                <td>@boat.closingDate</td>
                <td>@boat.reopeningDate</td>
            </tr>
        }
    </tbody>
</table>